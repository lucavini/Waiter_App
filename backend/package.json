    "docker:start": "sudo docker start mongo"
  },
  "dependencies": {
    "express": "^4.18.2",
    "link-module-alias": "^1.2.0",
    "module-alias": "^2.2.2",
    "mongoose": "^7.2.0"
  },
  "devDependencies": {
    "@types/module-alias": "^2.0.1",
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "eslint": "^8.41.0",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
