{
  "name": "waiterapp",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "postinstall": "link-module-alias",
    "docker:create": "sudo docker run --name mongo -p 27017:27017 -d mongo",
